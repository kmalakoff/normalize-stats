{
  "name": "normalize-stats",
  "version": "1.0.10",
  "description": "Create BigIntStats from fs.Stats for compatibility on earlier versions of Node.js",
  "keywords": [
    "dirent",
    "fs.Dirent",
    "stats",
    "fs.Stats",
    "compatibility",
    "es5"
  ],
  "homepage": "https://github.com/kmalakoff/normalize-stats",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/normalize-stats.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "main": "dist/cjs/index.cjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "fs-generate": "^1.9.8",
    "fs-stats-spys": "^1.1.8",
    "rimraf2": "^2.8.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
