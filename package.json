{
  "name": "normalize-stats",
  "version": "1.0.0",
  "description": "Create BigIntStats from fs.Stats for compatibility on earlier versions of Node.js",
  "main": "index.js",
  "engines": {
    "node": ">=0.8.0"
  },
  "scripts": {
    "test": "mocha-compat test/spec/**/*.test.js",
    "compat": "nvs 14,12,10,8,6,4,0.12,0.10,0.8 mocha-compat test/spec/**/*.test.js",
    "lint": "eslint .",
    "format": "prettier --write '**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/normalize-stats.git"
  },
  "keywords": [
    "dirent",
    "fs.Dirent",
    "stats",
    "fs.Stats",
    "compatibility",
    "es5"
  ],
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "license": "MIT",
  "homepage": "https://github.com/kmalakoff/normalize-stats",
  "dependencies": {
    "big.js": "^5.2.2",
    "jsbi-compat": "^0.2.0",
    "legacy-extends": "^0.4.0"
  },
  "devDependencies": {
    "async-each": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "end-with": "^1.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fs-generate": "^1.8.0",
    "fs-stats-spys": "^1.0.1",
    "is-safe-integer": "^2.0.0",
    "lodash.isdate": "^4.0.1",
    "mocha-compat": "^3.5.5",
    "nvs": "^0.2.0",
    "prettier": "^2.0.5",
    "rimraf": "^2.7.1"
  }
